        for y, row in enumerate(self.shape):
            for x, cell in enumerate(row):
                if cell:
                    pygame.draw.rect(screen, (255, 255, 255), 
                                     ((self.position[0] + x) * BLOCK_SIZE, 
                                      (self.position[1] + y) * BLOCK_SIZE, 
