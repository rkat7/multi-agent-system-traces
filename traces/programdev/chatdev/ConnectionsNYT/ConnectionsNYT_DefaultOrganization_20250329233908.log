import random
class Puzzle:
    def __init__(self, words, categories):
        '''
        Initializes the Puzzle with words and categories.
        '''
        self.words = words
        self.categories = categories
        self.selected_words = []
        self.correct_groups = []
    def shuffle_words(self):
        '''
        Shuffles the words in the puzzle.
        '''
        random.shuffle(self.words)
    def check_group(self, selected_words):
        '''
        Checks if the selected words form a correct group.
        Returns the category if correct, otherwise None.
        '''
        for category, words in self.categories.items():
            if set(selected_words) == set(words):
                return category
        return None
    def remove_correct_group(self, group):
        '''
