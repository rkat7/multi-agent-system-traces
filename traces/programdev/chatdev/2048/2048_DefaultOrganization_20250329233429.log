        while i < len(merged) - 1:
            if merged[i] == merged[i + 1]:
                merged[i] *= 2
                score += merged[i]
                merged[i + 1] = 0
                i += 1  # Skip the next tile since it has been merged
            i += 1
        merged = [i for i in merged if i != 0]
        return merged + [0] * (4 - len(merged)), score

